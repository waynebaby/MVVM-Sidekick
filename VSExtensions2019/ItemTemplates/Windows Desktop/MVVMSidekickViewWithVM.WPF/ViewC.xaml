<UserControl x:Class="$rootnamespace$.$safeitemname$"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Height="480" Width="640"
        xmlns:mvvm="clr-namespace:MVVMSidekick.Views;assembly=MVVMSidekick_net46"
        xmlns:vm="clr-namespace:$rootnamespace$.ViewModels"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
        mc:Ignorable="d">
    <Grid x:Name="grid" >
        <d:FrameworkElement.DataContext>
            <vm:$safeitemname$_Model/>
        </d:FrameworkElement.DataContext>
    </Grid>
</UserControl>
